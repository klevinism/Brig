let column be 10
let row be 10

for (let y : 0 ; y lower column; let y be y+1) do {
	
	if(y lower column/2) {
	
		for(let z be y; z lower row/2; let z be z+1) do {
			print " "
		}
	
		for (let x be 0; x is lower y; let x be x+1) do {
			print " *"
		}
	
	}else{
		for(let z be y+1; z greater row/2; let z be z-1) do {
			print " "
		}
	
		for (let x be y+1; x is lower row; let x be x+1) do {
			print " *"
		}
	}

println ""
}

	
